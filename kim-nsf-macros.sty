\definecolor{titlebg}{RGB}{100,22,72}

\newtcolorbox{taskbox}{
  arc=0pt,
  outer arc=0pt,
  colframe=titlebg,
  colback=titlebg!05,
  boxrule=0pt,       
  top=0pt,             % Set top margin
  bottom=0pt,          % Set bottom margin  
  overlay unbroken and first={
    \node[
      draw=titlebg,
      fill=titlebg,
      rotate=270,
      anchor=north west,
      text=white,
      font=\bfseries
    ]
  }
}

\newtcolorbox{tbox}[1][]{
  breakable,
  enhanced,
  arc=0pt,
  outer arc=0pt,
  colframe=titlebg,
  colback=titlebg!05,
  boxrule=0pt,       
  top=0.0pt,             % Set top margin
  bottom=0.0pt,          % Set bottom margin  
  overlay unbroken and first={
    \node[
      draw=titlebg,
      fill=titlebg,
      rotate=0,
      anchor=north west,
      text=white,
      font=\bfseries
    ]
    at (frame.north west)  
    {#1};
  }
}


